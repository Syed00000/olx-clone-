{
  "name": "olx-clone-backend",
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.ts"
    },
    {
      "src": "/health",
      "dest": "backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "backend/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}