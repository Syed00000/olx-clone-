{
  "name": "olx-clone-backend",
  "version": 2,
  "builds": [
    {
      "src": "backend/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "backend/index.ts": {
      "maxDuration": 30
    }
  }
}
